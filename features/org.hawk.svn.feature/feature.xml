<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.hawk.svn.feature"
      label="Hawk SVN Connector Feature"
      version="1.2.0.qualifier"
      provider-name="University of York">

   <description>
      Allows Hawk to monitor a SVN repository.
   </description>

   <copyright>
     Copyright (C) 2013-2018 University of York, Aston University.
   </copyright>

   <license url="https://www.eclipse.org/legal/epl-2.0/">
      Eclipse Public License v2.0 with GPLv3.0 Secondary License
   </license>

   <requires>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.hawk.core" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.tmatesoft.svnkit" version="1.8.6" match="greaterOrEqual"/>
      <import plugin="org.tmatesoft.sqljet" version="1.1.10" match="greaterOrEqual"/>
   </requires>

   <plugin
         id="org.hawk.svn"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
