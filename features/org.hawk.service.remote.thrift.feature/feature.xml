<?xml version="1.0" encoding="UTF-8"?>
<feature
    id="org.hawk.service.remote.thrift.feature"
    label="Thrift API integration for Hawk GUI Feature"
    version="1.2.0.qualifier"
    provider-name="University of York">

  <description>
    Provides the ability to use a remote Hawk index in the Hawk Eclipse GUI.
  </description>

  <copyright>
    Copyright (C) 2015-2018 University of York, Aston University.
  </copyright>

  <license url="https://www.eclipse.org/legal/epl-2.0/">
    Eclipse Public License - v2.0
  </license>

  <requires>
    <import plugin="org.hawk.core" version="1.0.0" match="greaterOrEqual"/>
    <import plugin="org.hawk.service.api" version="1.0.0" match="greaterOrEqual"/>
    <import plugin="org.apache.thrift" version="0.9.3" match="greaterOrEqual"/>
    <import plugin="org.hawk.core.dependencies" version="1.0.0" match="greaterOrEqual"/>
    <import plugin="org.eclipse.osgi"/>
    <import plugin="org.slf4j.api" version="1.7.2" match="greaterOrEqual"/>
    <import plugin="org.eclipse.core.runtime"/>
    <import plugin="org.hawk.service.artemis" version="1.0.0" match="greaterOrEqual"/>
    <import plugin="org.apache.httpcomponents.httpclient" version="4.3.6" match="greaterOrEqual"/>
    <import plugin="org.eclipse.swt"/>
    <import plugin="org.eclipse.jface"/>
    <import plugin="org.eclipse.ui"/>
    <import plugin="org.hawk.osgiserver"/>
    <import plugin="org.hawk.service.api.dt"/>
  </requires>

  <plugin
      id="org.hawk.service.remote.thrift"
      download-size="0"
      install-size="0"
      version="0.0.0"
      unpack="false"/>

</feature>
