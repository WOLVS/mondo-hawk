      Eclipse Public License - v2.0
   </license>

   <requires>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.hawk.core" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.epsilon.eol.engine" version="1.1.0" match="greaterOrEqual"/>
      <import plugin="org.hawk.graph" version="1.0.0" match="greaterOrEqual"/>
   </requires>

   <plugin
         id="org.hawk.epsilon"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
