   </copyright>

   <license url="https://www.eclipse.org/legal/epl-2.0/">
      Eclipse Public License - v2.0
   </license>

   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.jface.text"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.ui.editors"/>
      <import plugin="org.eclipse.ui.ide"/>
      <import plugin="org.eclipse.ui.forms"/>
      <import plugin="org.hawk.service.emf" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.hawk.service.api"/>
      <import plugin="org.eclipse.epsilon.dt.exeed" version="1.3.0" match="greaterOrEqual"/>
      <import plugin="org.apache.thrift" version="0.9.2" match="greaterOrEqual"/>
      <import plugin="org.hawk.emfresource" version="1.0.0" match="greaterOrEqual"/>
