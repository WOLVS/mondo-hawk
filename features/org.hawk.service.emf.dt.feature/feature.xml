      <import plugin="org.hawk.service.remote.thrift" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.apache.httpcomponents.httpclient" version="4.3.6" match="greaterOrEqual"/>
      <import plugin="com.google.guava" version="15.0.0" match="greaterOrEqual"/>
      <import plugin="org.hawk.service.api.dt"/>
   </requires>

   <plugin
         id="org.hawk.service.emf.dt"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
